{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "meshStack Documentation Hub",
  "colors": {
    "primary": "#5ecee8",
    "light": "#5ECEE8",
    "dark": "#5ECEE8"
  },
  "fonts": {
    "heading": {
      "family": "Montserrat Bold",
      "source": "https://mintlify-assets.b-cdn.net/fonts/montserrat-v25-latin-600.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "Montserrat",
      "source": "https://mintlify-assets.b-cdn.net/fonts/montserrat-v25-latin-regular.woff2",
      "format": "woff2"
    }
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "User Docs",
            "pages": [
              "userdocs/overview",
              "userdocs/marketplace-overview",
              "userdocs/workspace"
            ]
          },
          {
            "group": "Administrator Docs",
            "pages": [
              "admindocs/replication",
              "admindocs/onboarding"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Endpoints",
            "pages": [
              "api-reference/endpoint/endpoint1",
              "api-reference/endpoint/endpoint2",
              "api-reference/endpoint/endpoint3"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://cloudfoundation.org/",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://meshcloud.io/en/blog/",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://www.meshcloud.io/en/company/contact/"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Book Demo",
      "href": "https://www.meshcloud.io/en/book-demo/"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/meshstack",
      "github": "https://github.com/meshcloud",
      "linkedin": "https://www.linkedin.com/company/meshcloud"
    }
  }
}